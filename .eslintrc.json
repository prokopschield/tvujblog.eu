{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": ["plugin:lvksh/recommended"],
    "ignorePatterns": ["!**/*"],
    "plugins": ["lvksh"],
    "env": {
        "node": true
    },
    "rules": {
        "unicorn/prefer-at": "off",
        "unicorn/prefer-node-protocol": "off"
    }
}
